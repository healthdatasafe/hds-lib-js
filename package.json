{
  "name": "hds-lib",
  "version": "0.1.14",
  "description": "Health Data Safe - Library",
  "scripts": {
    "test": "NODE_ENV=test mocha tests --test-reporter=spec",
    "test:coverage": "NODE_ENV=test c8 mocha tests --test-reporter=spec",
    "build": "npm run build:ts; npm run build:webpack",
    "build:ts": "tsc",
    "build:webpack": "webpack",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:change": "eslint $(git diff --name-only HEAD | grep -E '\\.(js|jsx|ts|tsx)$' | xargs)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/healthdatasafe/hds-lib-js.git"
  },
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "keywords": [
    "HDS",
    "Health",
    "Data",
    "Safe"
  ],
  "author": "HDS",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/healthdatasafe/hds-lib-js/issues"
  },
  "homepage": "https://github.com/healthdatasafe/hds-lib-js#readme",
  "devDependencies": {
    "assert": "^2.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "mocha": "^11.5.0",
    "neostandard": "^0.12.0",
    "process": "^0.11.10",
    "ts-loader": "^9.5.4",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@pryv/monitor": "3.0.1",
    "@pryv/socket.io": "3.0.1",
    "c8": "^10.1.3",
    "events": "^3.3.0",
    "pryv": "3.0.1",
    "short-unique-id": "^5.3.2"
  }
}
